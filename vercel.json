{
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": false
  },
  "rewrites": [
    { "source": "/about", "destination": "/about.html" },
    { "source": "/location", "destination": "/location.html" },
    { "source": "/insurance", "destination": "/insurance.html" },
    { "source": "/travel-terms", "destination": "/travel-terms.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/privacy", "destination": "/privacy.html" }
  ],
  "headers": [
    {
      "source": "/cebu-bohol",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/bali-resorts",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/lombok-resorts",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
